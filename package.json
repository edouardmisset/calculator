{
  "name": "calculator",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prebuild": "bun run tsc",
    "build": "vite build",
    "precommit": "bun run tsc && bun run format && bun run test:once && bun run eslint --fix && bun run prettier --write",
    "predeploy": "vite build",
    "deploy": "gh-pages -d dist",
    "dev": "bunx --bun vite",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "lint": "eslint ./src/ --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "bun run lint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:once": "vitest --run",
    "test:coverage": "bun run test:once --coverage",
    "test:e2e": "bun run cypress run",
    "test:all": "bun run test:once && bun run test:e2e",
    "cypress:open": "cypress open",
    "prepare": "husky",
    "release": "npx semantic-release",
    "reset": "rm -rf node_modules bun.lockb && bun install"
  },
  "dependencies": {
    "jest-styled-components": "^7.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.11",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.4.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "bun-types": "^1.1.12",
    "cypress": "^13.10.0",
    "devmoji": "^2.3.0",
    "eslint": "^9.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "semantic-release": "^24.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
