{
  "name": "calculator",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prebuild": "bun run tsc",
    "build": "vite build",
    "check": "bun run tsc && bun run lint:fix && bun run format && bun run test:once",
    "precommit": "bun run check",
    "predeploy": "vite build",
    "deploy": "gh-pages -d dist",
    "dev": "bunx --bun vite",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --cache --config ./.prettierrc.json",
    "lint": "eslint ./src --report-unused-disable-directives --cache --max-warnings 0",
    "lint:fix": "bun run lint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:once": "vitest --run",
    "test:coverage": "bun run test:once --coverage",
    "test:e2e": "bun run cypress run",
    "test:all": "bun run test:once && bun run test:e2e",
    "cypress:open": "cypress open",
    "prepare": "husky",
    "release": "npx semantic-release",
    "reset": "rm -rf node_modules bun.lock && bun install --save-text-lockfile"
  },
  "dependencies": {
    "jest-styled-components": "^7.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.14",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.19.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "bun-types": "^1.2.2",
    "cypress": "^14.0.1",
    "devmoji": "^2.3.0",
    "eslint": "^9.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.0.11",
    "vitest": "^3.0.5"
  }
}